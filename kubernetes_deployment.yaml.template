kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: hello-world
  namespace: default
  labels:
    run: hello-world
spec:
  replicas: 2
  template:
    metadata:
      labels:
        run: hello-world
    spec:
      containers:
      - name: hello-world
        image: gcr.io/sym-dev-plat/hello-world:0.1.0
        imagePullPolicy: Always
        ports:
        - containerPort: 8080
          protocol: TCP